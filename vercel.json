{
  "version": 2,
  "name": "music-album",
  "builds": [
    {
      "src": "/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**"],
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
