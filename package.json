{
  "name": "ecommerce-vc-api",
  "description": "Ecommerce API made by Victor Canas",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "postbuild": "node ace swagger:generate && cp -a docs/ build/docs",
    "start": "node build/server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "engines": {
    "node": "18.12.1",
    "npm": "8.19.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.7.0",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "@types/braintree": "^3.3.3",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "pino-pretty": "^8.0.0",
    "prettier": "^2.7.0",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.1",
    "@adonisjs/bouncer": "^2.3.0",
    "@adonisjs/core": "^5.8.3",
    "@adonisjs/drive-s3": "^1.3.1",
    "@adonisjs/i18n": "^1.5.6",
    "@adonisjs/lucid": "^18.0.1",
    "@adonisjs/mail": "^8.1.2",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/view": "^6.2.0",
    "@types/url-parse": "^1.4.8",
    "adonis5-swagger": "^1.4.1",
    "axios": "^1.1.3",
    "braintree": "^3.12.0",
    "luxon": "^2.4.0",
    "pg": "^8.7.3",
    "phc-argon2": "^1.1.3",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "url-parse": "^1.5.10"
  }
}
